  <body class="no-js">
    <header role="banner" id="global-header">
      <div class="header-wrapper">
        <div class="header-global">
          <div class="header-logo">
            <a href="https://www.gov.uk/" title="Go to the GOV.UK homepage" id="logo" class="content">
              <img src="public/assets/images/gov.uk_logotype_crown_invert_trans.png" width="35" height="31" alt=""> GOV.UK
            </a>
          </div>
        </div>
      </div>
    </header>

    <main id="wrapper">
      <div class="comp-lib-content">

        <div class="service-info">
          <div class="alpha-banner">
            <p>
              <strong class="phase-tag">ALPHA</strong><span>This is a trial service.</span>
            </p>
          </div>
          <div class="logo">
            <span class="organisation-logo organisation-logo-medium">HM Revenue &amp; Customs</span>
          </div>
        </div>
        <h1 class="heading-48">Component Library</h1>
        <div class="alert alert--info">
          <p class="alert__message">
            This component library is a visual representation of the styles used to build HMRC's services.
          </p>
          <h3>Questions and Issues</h3>
          <p class="alert__message">
          If you have questions about how to use either a component or the library, or would like to report an issue then please get in touch using one of our
            <a href="http://hmrc.github.io/assets-frontend#support-and-feedback">support channels</a>.
          </p>
        </div>
        <div class="comp-lib">

          <aside class="comp-lib-sidebar">
            <nav>
              <ul>
                <li class="comp-lib-menu-item{{#if homepage}} comp-lib-menu-item--highlighted{{/if}}">
                  <a href="./" class="comp-lib-menu-link">Home</a>
                </li>
                {{#each menu}}
                  <li class="comp-lib-menu-item{{#if isActive}} comp-lib-menu-item--highlighted{{/if}}">
                    <a href="section-{{referenceURI}}.html" class="comp-lib-menu-link">{{header}}</a>
                    {{#if isActive}}
                      {{#if children}}
                        <ul>
                        {{#each children}}
                            <li class="comp-lib-menu-item--indented">
                              <a href="section-{{../referenceURI}}.html#kssref-{{referenceURI}}">
                                {{header}}
                              </a>
                            </li>
                          {{/each}}
                        </ul>
                      {{/if}}
                    {{/if}}
                  </li>
                {{/each}}
              </ul>
            </nav>
          </aside>
          <div id="content">
            <article class="comp-lib-main">
  {{#if template.isHomepage}}
    {{#if homepage}}
      <div class="comp-lib-pattern-markdown">
        {{{homepage}}}
      </div>
    {{/if}}
  {{else}}

  {{#each sections}}
    <{{#if @first}}div{{else}}section{{/if}} id="kssref-{{referenceURI}}">
      <div>
        <h{{depth}}>
          <a href="#kssref-{{referenceURI}}" class="comp-lib-menu-link">
            {{header}}
          </a>
        </h{{depth}}>

        {{#ifCond depth 1}}
          {{#if discussion}}
            <div class="alert alert--info" role="alert">
              <p class="alert__message">Discuss this component on <a href="{{discussion}}">Hackpad</a></p>
            </div>
          {{/if}}
        {{/ifCond}}

        {{#if parameters}}
          <ul>
          {{#each parameters}}
            <li>
              <code>{{name}}</code>
              <div>{{{description}}}</div>
            </li>
          {{/each}}
          </ul>
        {{/if}}

        {{#if description}}
          <div class="comp-lib-description
                                comp-lib-pattern-markdown
                                {{#if experimental}} comp-lib-description--experimental{{/if}}
            {{#if deprecated}} comp-lib-description--deprecated{{/if}}">
            <h2 class="comp-lib-warning">
              {{#if experimental}}Experimental!{{/if}}
              {{#if deprecated}}Deprecated!{{/if}}
            </h2>
            {{#removePrefix}}{{{description}}}{{/removePrefix}}
          </div>
        {{/if}}
      </div>

      {{#if markup}}
      <div>
        <div class="comp-lib-pattern-example">
          <div class="comp-lib-pattern-component comp-lib-clearfix{{#ifCond background 'dark'}} comp-lib-pattern-component--dark{{/ifCond}}">
            {{{markup}}}
          </div>
          <div class="comp-lib-code-example">
            <pre class="comp-lib-pre"><code class="comp-lib-code html">{{markup}}</code></pre>
          </div>
        </div>
        {{#each modifiers}}
            <div class="comp-lib-pattern-example">
              <h2>{{name}}</h2>
              <div class="comp-lib-description">
                {{{description}}}
              </div>
              <div class="comp-lib-pattern-component comp-lib-clearfix{{#ifCond background 'dark'}} comp-lib-pattern-component--dark{{/ifCond}}">
                {{{markup}}}
              </div>
              <div class="comp-lib-code-example">
                <pre class="comp-lib-pre"><code class="comp-lib-code html">{{markup}}</code></pre>
              </div>
            </div>
          {{/each}}
      </div>
      {{/if}}

    </{{#if @first}}div{{else}}section{{/if}}>

    {{#if [example data]}}
      <!-- data associated with a component -->
      <script type="text/javascript">
          {{{[example data]}}}
      </script>
    {{/if}}
  {{/each}}
{{/if}}
</article>
          </div>
</div>
    </div>
